{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/log-.txt",
          "rollingInterval": "Day"
        }
        //"Args": {
        //  "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
        //  "path": "./Logs/log-.json",
        //  "rollingInterval": "Day"
        //}
      }
    ]
  },
  "ConnectionStrings": {
    "EPManifestContext": "Data Source= (localdb)\\MSSQLLocalDB; Initial Catalog=EPManifest"
  },
  "AllowedHosts": "*"
}