@page "/help"

<MudContainer MaxWidth="MaxWidth.ExtraLarge" Class="mt-16">
    <MudText Typo="Typo.h3" GutterBottom="true">Help</MudText>
    <MudCard>
        <MudSkeleton SkeletonType="SkeletonType.Rectangle" Height="200px" Animation="Animation.Wave" />
        <MudCardContent>
            <MudSkeleton Width="30%" Height="42px;" Animation="Animation.Wave" />
            <MudSkeleton Width="80%" Animation="Animation.Wave" />
            <MudSkeleton Width="100%" Animation="Animation.Wave" />
        </MudCardContent>
        <MudCardActions>
            <MudSkeleton Width="64px" Height="40px" Class="ml-2" Animation="Animation.Wave" />
            <MudSkeleton Width="105px" Height="40px" Class="ml-3" Animation="Animation.Wave" />
        </MudCardActions>
    </MudCard>
</MudContainer>

